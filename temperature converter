#include <iostream>
using namespace std;

float toCelsius(float fahrenheit)
{
    float celsius = (5.0/9.0*(fahrenheit-32.0));
    return celsius;
}

int main()
{
    cout << "fahrenheit to celsius converter." << endl;
    cout << endl;
    for (int i = 60.0; i <= 80.0; i++) {
        cout << "fahrenheit " << " celsius " << endl;
        cout << i << "          " << toCelsius(i) << endl;
    }
    return 0;
}
